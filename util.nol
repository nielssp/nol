let & = \a b -> if a then b else false

let | = \a b -> if a then true else b

let % = \a b -> a - b * a / b

let not = \x -> if x then false else true

let odd = \x -> x % 2

let even = \x -> not $ odd x

let abs =  \x -> if x < 0 then neg x else x

let $ = \f x -> f x

let $> = \x f -> f x

let . = \f g x -> f $ g x

let .> = \f g x -> g $ f x

let flip = \f x y -> f y x

